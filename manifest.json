{
  "manifest_version": 3,
  "name": "图标尺寸生成器",
  "version": "1.0",
  "description": "快速生成多种尺寸的图标文件，支持128x128、48x48、32x32、16x16尺寸",
  "permissions": ["storage"],
  "action": {
    "default_popup": "popup.html",
    "default_title": "图标尺寸生成器"
  },
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}
